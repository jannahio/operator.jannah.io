edit ansible config
    ansible/ansible.cfg
        ansible_python_interpreter = jannah-operator/bin/python3.9


cd jannah-operator;
source ./boot.sh;
cd ansible/
ansible-playbook -i inventory/ site.yml -vvvv --connection=local --vault-id defaultpass@$ANSIBLE_VAULT_DEFAULT_PASS_FILE --tags d1d2-install-operator-sdk